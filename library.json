{
  "name": "Micro-RTSP",
  "keywords": "esp32, camera, esp32-cam, rtsp",
  "description": "A small/efficient RTSP server for ESP32 and other micros",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/rogersanctus/Micro-RTSP.git"
  },
  "authors":
  [
    {
      "name": "Kevin Hester",
      "email": "kevinh@geeksville.com",
      "url": "https://github.com/geeksville",
      "maintainer": true
    }
  ],
  "version": "0.1.7",
  "frameworks": ["arduino", "espidf"],
  "platforms": "*",
  "build": {
    "flags": [
      "-Isrc",
      "-fno-rtti"
    ],
    "includeDir": "./src",
    "srcDir": "./src",
    "srcFilter": ["-<*>", "+<src>"]
  }
}
